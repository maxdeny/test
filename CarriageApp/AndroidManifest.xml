    <application
        android:name="com.beatle.lg.carriage.MApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.beatle.lg.carriage.act.ActLauch"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/no_title_bar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.beatle.lg.carriage.act.ActLogin"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/no_title_bar" >
        </activity>
        <activity
            android:name="com.beatle.lg.carriage.act.ActRegisterFirst"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/no_title_bar" >
        </activity>
        <activity
            android:name="com.beatle.lg.carriage.act.ActRegisterSecond"
            android:label="@string/app_name"
